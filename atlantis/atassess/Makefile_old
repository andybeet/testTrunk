include ../Makefile.inc

#WARN +=-Werror
OBJECTS = atclassical.o atindices.o atsample.o atsurvey.o \
	atassessprmIO.o atdatareading.o atmemory.o atsamplesetup.o \
	atwriting.o atattribute.o atdiet.o atnetwork.o atsolve.o  \
	outputAssessArrays.o atAssessModule.o	
